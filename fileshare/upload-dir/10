lada(arany). %lada(X) :- X=arany.
lada(ezust).
lada(olom).

festo(velencei).
festo(firenzei).

arany1(Hely) :- lada(Hely), Hely\=arany. 
arany2(Festo):- Festo=velencei. %arany2(velencei). ugynazt jelenti
ezust1(Hely) :- lada(Hely), Hely\=arany.
ezust2(Festo):- Festo=firenzei.
olom1(Hely) :- lada(Hely), Hely\=olom. 
olom2(Hely) :- Hely=ezust.

legf1hamis_arany(Hely,Festo) :- arany1(Hely); arany2(Festo).
legf1hamis_ezust(Hely,Festo) :- ezust1(Hely); ezust2(Festo).
legf1hamis_olom(Hely) :- olom1(Hely); olom2(Hely).

:- legf1hamis_arany(Hely,Festo), legf1hamis_ezust(Hely,Festo), 
    legf1hamis_olom(Hely), write(Hely).
