#!/bin/bash

TEMPERATURE=15

for RETRY in 1 2 3 4 5 6
do
        TOKEN=`curl -s --request POST 'https://portal.icy.nl/login' --data 'username=tothbalint@caesar.elte.hu&password=jelszo123' | awk -F":" '{print $12}' | awk -F'"' '{print $2}'`
        
        RESULT=`curl -s -H "Session-token:$TOKEN" --request POST 'https://portal.icy.nl/data' --data "uid={UID}&temperature1=$TEMPERATURE" | tr -d '{}"' | awk -F":" '{print $4}'`
        [ $RESULT = OK ] && break
        sleep 300
done


curl -s --request POST 'https://portal.icy.nl/login' --data 'username=tothbalint@caesar.elte.hu&password=jelszo1as23' | awk -F":" '{print $12}' | awk -F'"' '{print $2}'

curl -X POST --data 'username=nevembalint&password=anonymus' 'https://17.134.127.250'

